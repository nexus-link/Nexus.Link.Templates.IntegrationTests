@{
    ViewData["Title"] = "Events";
}

<script src="~/js/signalr/dist/browser/signalr.min.js"></script>

<script>
    var connection = new signalR.HubConnectionBuilder().withUrl("/testsHub").build();

    connection.on("Apa", function (labels, data) {
        console.log("Apa", labels, data);
    });

    connection.start().then(function () {})
        .catch(function (err) {
            return console.error("Connection error", err.toString());
        });

</script>